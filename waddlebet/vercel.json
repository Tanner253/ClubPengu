{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/nft/:path*",
      "destination": "https://clubpengu-server.onrender.com/api/nft/:path*"
    },
    {
      "source": "/ref/:code*",
      "destination": "/index.html"
    },
    {
      "source": "/((?!assets|favicon|api|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
